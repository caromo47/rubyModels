<h1>Listing <%= pluralize(@dojos.count, "Dojo")%></h1>
<% if flash[:notice] %>
	<p><%= flash[:notice] %></p>
<% end %>
<table>
	<tr>
		<td>Coding Dojo Branch</td>
		<td>Street</td>
		<td>City</td>
		<td>State</td>
		<td>Actions</td>
	</tr>
	<tr>
		<% @dojos.each do |dojo| %>
		<td><%= dojo.branch %></td>
		<td><%= dojo.street %></td>
		<td><%= dojo.city %></td>
		<td><%= dojo.state %></td>
		<td>
			<%= link_to 'SHOW', "dojos/#{dojo.id}" %>|
			<%= link_to "EDIT", "dojos/#{dojo.id}/edit" %>|
			<%= link_to "DELETE", "dojos/#{dojo.id}/destroy", :method=>:delete, :class=>:destroy, data: { method: :delete, confirm: "Are you sure?" } %>
		</td>
	</tr>
		<% end %>
</table>
<%= link_to "New Dojo", '/dojos/new' %>
